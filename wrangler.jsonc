  "vars": {
    "OPENAI_API_KEY": "<YOUR_OPENAI_API_KEY>",
    "PRODIGI_API_KEY": "<YOUR_PRODIGI_API_KEY>",
    "STRIPE_API_KEY": "<YOUR_STRIPE_API_KEY>"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}